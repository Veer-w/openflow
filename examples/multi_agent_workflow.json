{
  "id": "multi-agent-flow",
  "name": "Multi Agent Flow",
  "nodes": [
    {
      "id": "n1",
      "type": "manual_trigger",
      "params": {}
    },
    {
      "id": "n2",
      "type": "langgraph_agent",
      "params": {
        "model": "qwen2.5:1.5b",
        "input_field": "message",
        "num_ctx": 1024,
        "num_predict": 128,
        "temperature": 0.2,
        "max_tool_calls": 4,
        "agents": [
          {
            "name": "researcher",
            "system_prompt": "Find relevant facts and references with tools before answering.",
            "tools": ["tavily_search"]
          },
          {
            "name": "synthesizer",
            "system_prompt": "Synthesize previous agent outputs into a concise final answer.",
            "tools": ["calculator", "utc_time"]
          }
        ]
      }
    },
    {
      "id": "n3",
      "type": "template",
      "params": {
        "template": "Multi-agent output => {{json}}"
      }
    }
  ],
  "edges": {
    "n1": ["n2"],
    "n2": ["n3"]
  },
  "active": true
}
